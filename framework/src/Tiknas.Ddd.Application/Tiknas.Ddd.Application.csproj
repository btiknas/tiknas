    <ProjectReference Include="..\Tiknas.Http.Abstractions\Tiknas.Http.Abstractions.csproj" />
    <ProjectReference Include="..\Tiknas.Localization\Tiknas.Localization.csproj" />
    <ProjectReference Include="..\Tiknas.ObjectMapping\Tiknas.ObjectMapping.csproj" />
    <ProjectReference Include="..\Tiknas.Security\Tiknas.Security.csproj" />
    <ProjectReference Include="..\Tiknas.Settings\Tiknas.Settings.csproj" />
    <ProjectReference Include="..\Tiknas.Validation\Tiknas.Validation.csproj" />
  </ItemGroup>

</Project>
