@using Tiknas.AspNetCore.Mvc.UI.Theme.Shared.Demo.Views.Components.Themes.Shared.Demos.ProgressBarsDemo

<tiknas-component-demo-section title="Example" view-path="@ProgressBarsDemoViewComponent.ViewPath">
    <tiknas-progress-bar value="70" />

    <tiknas-progress-bar type="Warning" value="25"> %25 </tiknas-progress-bar>

    <tiknas-progress-bar type="Success" value="40" strip="true" />

    <tiknas-progress-bar type="Dark" value="10" min-value="5" max-value="15" strip="true"> %50 </tiknas-progress-bar>

    <tiknas-progress-group>
        <tiknas-progress-part type="Success" value="25" />
        <tiknas-progress-part type="Danger" value="10" strip="true"> %10 </tiknas-progress-part>
        <tiknas-progress-part type="Primary" value="50" animation="true" strip="true" />
    </tiknas-progress-group>
</tiknas-component-demo-section>
