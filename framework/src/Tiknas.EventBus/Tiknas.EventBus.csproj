    <ProjectReference Include="..\Tiknas.Guids\Tiknas.Guids.csproj" />
    <ProjectReference Include="..\Tiknas.Json\Tiknas.Json.csproj" />
    <ProjectReference Include="..\Tiknas.MultiTenancy\Tiknas.MultiTenancy.csproj" />
  </ItemGroup>

</Project>
